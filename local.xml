    <simplecheckout_index_index>
        <reference name="head">
            <action method="removeItem"><type>js</type><name>magebuzz/simplecheckout/simplecheckout.js</name></action>
            <action method="removeItem"><type>skin_js</type><name>magebuzz/simplecheckout/js/jquery-1.10.1.min.js</name></action>
            <action method="removeItem"><type>skin_js</type><name>magebuzz/simplecheckout/js/jquery-ui.js</name></action>
            <action method="removeItem"><type>skin_js</type><name>magebuzz/simplecheckout/js/jquery.fancybox.js</name></action>
        </reference>

        <reference name="before_body_end">
            <action method="addJs"><script>magebuzz/simplecheckout/simplecheckout.js</script></action>
            <action method="addItem" ifconfig="simplecheckout/general/include_js">
                <type>skin_js</type>
                <name>magebuzz/simplecheckout/js/jquery-1.10.1.min.js</name>
            </action>
            <action method="addItem" ifconfig="simplecheckout/deliverydate/enable_delivery">
                <type>skin_js</type>
                <name>magebuzz/simplecheckout/js/jquery-ui.js</name>
            </action>
            <action method="addItem">
                <type>skin_js</type>
                <name>magebuzz/simplecheckout/js/jquery.fancybox.js</name>
            </action>
        </reference>
    </simplecheckout_index_index>

    <review_product_list translate="label">
        <reference name="head">
            <action method="removeItem"><type>js</type><name>varien/product.js</name></action>
            <action method="removeItem"><type>js</type><name>varien/product_options.js</name></action>
            <action method="removeItem"><type>js</type><name>varien/configurable.js</name></action>
        </reference>
        <reference name="before_body_end">
            <action method="addJs"><script>varien/product.js</script></action>
            <action method="addJs"><script>varien/product_options.js</script></action>
            <action method="addJs"><script>varien/configurable.js</script></action>
        </reference>
    </review_product_list>

    <alothemesdefault>
        <reference name="head">
            <action method="removeItem"><type>js</type><name>varien/product.js</name></action>
